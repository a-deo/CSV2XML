<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>logo\favicon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="apple-touch-icon.png" />
    <None Remove="favicon-16x16.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="logo\favicon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="apple-touch-icon.png" />
    <Resource Include="favicon-16x16.png" />
  </ItemGroup>

</Project>
